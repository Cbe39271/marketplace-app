    "cross-env": "7.0.3",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "ethers": "5.4.5",
    "framer-motion": "4.1.17",
    "graphql": "^16.5.0",
    "husky": "5.2.0",
    "i18next": "21.6.16",
    "i18next-browser-languagedetector": "6.1.4",
    "i18next-http-backend": "1.4.0",
    "identity-obj-proxy": "^3.0.0",
