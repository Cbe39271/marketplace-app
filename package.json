    "react-countup": "6.2.0",
    "react-dom": "17.0.2",
    "react-i18next": "11.11.4",
    "react-markdown": "7.0.1",
    "react-popper": "2.2.5",
    "react-redux": "7.2.8",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-transition-group": "4.4.2",
    "redux-localstorage-simple": "2.4.1",
    "redux-logger": "3.0.6",
    "styled-components": "5.3.5",
    "styled-system": "5.1.5",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "4.3.5",
