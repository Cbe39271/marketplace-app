  "devDependencies": {
    "babel-plugin-styled-components": "2.0.7",
    "eslint-plugin-import": "^2.26.0",
    "jest-environment-jsdom": "^29.1.2",
    "locize-cli": "7.10.1"
  },
  "resolutions": {
    "framer-motion": "4.1.17"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not IE > 0",
      "not op_mini all",
      "not dead",
      "last 2 Edge major versions",
      "last 1 Chrome version",
      "last 2 iOS major versions",
      "last 1 Firefox version",
      "not iOS 12.5",
      "Chrome 99"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ]
  },
  "jest": {
    "rootDir": ".",
    "transform": {
      "^.+\\.tsx$": "ts-jest",
      "^.+\\.ts$": "ts-jest"
    },
    "moduleNameMapper": {
      "@src/(.*)": "<rootDir>/src/$1",
      "@src": "<rootDir>/src/index",
      "@client/(.*)": "<rootDir>/src/client/$1",
      "@client": "<rootDir>/src/client/index",
      "@assets/(.*)": "<rootDir>/src/client/assets/$1",
      "@assets": "<rootDir>/src/client/assets/index",
      "@components/(.*)": "<rootDir>/src/client/components/$1",
      "@components": "<rootDir>/src/client/components/index",
      "@containers/(.*)": "<rootDir>/src/client/containers/$1",
      "@containers": "<rootDir>/src/client/containers/index",
      "@context": "<rootDir>/src/client/context/index",
      "@hooks/(.*)": "<rootDir>/src/client/hooks/$1",
      "@hooks": "<rootDir>/src/client/hooks/index",
      "@i18n/(.*)": "<rootDir>/src/client/i18n/$1",
      "@i18n": "<rootDir>/src/client/i18n/index",
      "@routes/(.*)": "<rootDir>/src/client/routes/$1",
      "@routes": "<rootDir>/src/client/routes/index",
      "@themes/(.*)": "<rootDir>/src/client/themes/$1",
      "@themes": "<rootDir>/src/client/themes/index",
      "@config/(.*)": "<rootDir>/src/config/$1",
      "@config": "<rootDir>/src/config/index",
      "@core/(.*)": "<rootDir>/src/core/$1",
      "@core": "<rootDir>/src/core/index",
      "@container": "<rootDir>/src/core/container/index",
      "@frameworks/(.*)": "<rootDir>/src/core/frameworks/$1",
      "@frameworks": "<rootDir>/src/core/frameworks/index",
      "@services/(.*)": "<rootDir>/src/core/services/$1",
      "@services": "<rootDir>/src/core/services/index",
      "@providers/(.*)": "<rootDir>/src/core/providers/$1",
      "@providers": "<rootDir>/src/core/providers/index",
      "@store": "<rootDir>/src/core/store/index",
      "@useCases/(.*)": "<rootDir>/src/core/useCases/$1",
      "@types": "<rootDir>/src/core/types/index",
      "@scripts": "<rootDir>/src/scripts/index",
      "@utils/(.*)": "<rootDir>/src/utils/$1",
      "@utils": "<rootDir>/src/utils/index",
      "\\.(jpg|jpeg|png|css)$": "identity-obj-proxy"
    }
  }
